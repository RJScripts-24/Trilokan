{
  "name": "digital-trust-api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for Digital Trust & Cyber Fraud Detection System",
  "main": "src/index.js",
  "repository": "https://github.com/yourusername/digital-trust-gateway.git",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watch",
    "coverage": "jest -i --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "db:migrate": "migrate-mongo up -f db/config.js",
    "db:migrate:down": "migrate-mongo down -f db/config.js",
    "db:status": "migrate-mongo status -f db/config.js"
  },
  "keywords": [
    "node",
    "express",
    "digital-trust",
    "fraud-detection",
    "forensics",
    "api-gateway"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.0.0",
    "http-status": "^1.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "faker": "^5.5.3",
    "jest": "^29.7.0",
    "migrate-mongo": "^11.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  }
}