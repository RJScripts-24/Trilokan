    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.0",
    "sequelize": "^6.35.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4",
    "yamljs": "^0.3.0"
